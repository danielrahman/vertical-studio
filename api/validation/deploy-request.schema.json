{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["jobId", "target"],
  "properties": {
    "jobId": {
      "type": "string",
      "minLength": 1
    },
    "target": {
      "type": "string",
      "enum": ["local", "vercel", "s3-cloudflare"]
    },
    "domain": {
      "type": "string",
      "minLength": 1
    }
  }
}
