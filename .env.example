# Runtime
NODE_ENV=development
PORT=3000
VERTICAL_RUNTIME_DIR=.runtime
VERTICAL_OUTPUT_ROOT=build-output/jobs
VERTICAL_WORKER_POLL_MS=500

# API auth
# In production this should stay enabled.
VERTICAL_REQUIRE_AUTH=false
# Prefer a comma-separated key list for multi-key rotation.
VERTICAL_API_KEYS=dev-local-key
# Optional single-key shortcut:
# VERTICAL_API_KEY=dev-local-key

# URL config
VERTICAL_PREVIEW_BASE_URL=http://localhost:3001/preview
VERTICAL_UI_BASE_URL=http://localhost:3001
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000

# Secrets management
# Use a strong value in real environments (32+ bytes base64 or 64-char hex).
VERTICAL_SECRET_MASTER_KEY=
# Optional inline JSON secret map:
# VERTICAL_SECRET_MAP_JSON={"captcha.2captcha":"<key>"}
# Optional file-based secret map:
# VERTICAL_SECRET_MAP_FILE=config/secrets.json
# Optional per-ref env fallbacks (no map file needed):
# VERTICAL_SECRET_REF_CAPTCHA_2CAPTCHA=...
# VERTICAL_SECRET_REF_OFFSITE_SERPAPI=...
# VERTICAL_SECRET_REF_AUTH_CLIENT={"username":"user@example.com","password":"secret"}

# External providers
OPENAI_API_KEY=
EXA_API_KEY=
SERPAPI_API_KEY=
COMPANY_DATA_API_KEY=
SOCIAL_ENRICH_API_KEY=
# Recommended default captcha provider for this project: 2captcha
CAPTCHA_API_KEY=

# Deploy providers
VERCEL_TOKEN=
VERCEL_PROJECT_ID=
VERCEL_TEAM_ID=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=
CLOUDFLARE_CDN_BASE_URL=
